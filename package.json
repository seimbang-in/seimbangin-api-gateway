{
  "name": "seimbangin-be",
  "version": "1.0.0",
  "description": "seimbang.in backend ",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "npm run clean && tsc -p tsconfig.json && npm run build:css:prod && npm run copy:assets",
    "build:css": "npx postcss ./src/app.css -o ./public/index.css --watch",
    "build:css:prod": "npx postcss ./src/app.css -o ./public/index.css",
    "copy:assets": "ts-node scripts/copy-assets",
    "clean": "rimraf dist .logs",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "studio": "drizzle-kit studio --config drizzle.config.ts",
    "generate": "drizzle-kit generate",
    "migrate": "tsx src/db/migrate.ts",
    "vercel:dev": "vercel dev --listen 5005",
    "deploy:vercel": "vercel build && vercel --prod",
    "build:vercel": "npm run build && vercel build && vercel --prod"
  },
  "author": "figomager",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.13.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express-session": "^1.18.0",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^3.2.23",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.41.0",
    "drizzle-seed": "^0.1.3",
    "express": "^4.21.1",
    "express-handlebars": "^8.0.1",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "openai": "^4.77.0",
    "postcss-cli": "^11.0.0",
    "uuid": "^11.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.8.1",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "drizzle-kit": "^0.30.6",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1",
    "shelljs": "^0.8.5",
    "tailwindcss": "^3.4.16",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}